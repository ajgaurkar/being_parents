        android:gravity="center"
        android:padding="5dp"
        android:text="Version"
        android:textColor="@color/colorAccent"
        android:textStyle="bold" />

    <ImageView
        android:id="@+id/imageView52"
        android:layout_width="180dp"
        android:layout_height="180dp"
