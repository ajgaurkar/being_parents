        android:orientation="vertical"
        android:weightSum="20">

        <View
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1.8" />

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="17">


            <ImageView
                android:id="@+id/trivendrumChartLeftNavigateBtn"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:background="?android:attr/selectableItemBackground"
                android:src="@drawable/back_96"
                android:layout_alignTop="@+id/trivendrumChartRightNavigateBtn"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true" />

            <ImageView
                android:id="@+id/trivendrumChartRightNavigateBtn"
                android:layout_width="50dp"
                android:background="?android:attr/selectableItemBackground"
                android:layout_height="50dp"
                android:src="@drawable/front_96"
                android:layout_centerVertical="true"
                android:layout_alignParentRight="true"
                android:layout_alignParentEnd="true" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_alignEnd="@+id/trivendrumChartRightNavigateBtn"
                android:layout_alignParentLeft="true"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:layout_alignRight="@+id/trivendrumChartRightNavigateBtn"
                android:orientation="horizontal"
                android:weightSum="260">

                <View
                    android:id="@+id/pointer"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:background="#00634e5b" />

                <View
                    android:id="@+id/pointerColorSupporter"
                    android:layout_width="2dp"
                    android:layout_height="match_parent"
                    android:background="#ff0099" />

            </LinearLayout>
        </RelativeLayout>

        <View
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1" />
    </LinearLayout>

    <com.google.android.gms.ads.AdView
        android:id="@+id/trivendrum_right_AdView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:visibility="gone"
        android:layout_alignParentRight="true"
        ads:adSize="SMART_BANNER"
        ads:adUnitId="@string/appbabybanner" />

    <com.google.android.gms.ads.AdView
        android:id="@+id/trivendrum_left_AdView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:visibility="gone"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        ads:adSize="SMART_BANNER"
        ads:adUnitId="@string/appbabybanner" />
</RelativeLayout>